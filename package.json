{
  "name": "intelligent-engineering-platform",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run dev --workspace=@intelligent-engineering/frontend",
    "dev:backend": "npm run dev --workspace=@intelligent-engineering/backend",
    "build": "npm run build --workspaces",
    "build:frontend": "npm run build --workspace=@intelligent-engineering/frontend",
    "build:backend": "npm run build --workspace=@intelligent-engineering/backend",
    "start": "npm run start --workspace=@intelligent-engineering/backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "mime-db": "^1.54.0",
    "pg-int8": "^1.0.1"
  }
}
